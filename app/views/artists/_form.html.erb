<%= link_to "<<<HOME", admins_path %>
<h1>Artist</h1>

<%= form_with model: @artist do |f| %>
 <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :genre %>
  <%= f.text_field :genre %>

  <%= f.submit %>
<% end %>

<% if @artist.id %>
<p><%= link_to 'New Song', new_artist_song_path(@artist) %> <%= link_to 'Edit Songs', artist_songs_path(@artist) %> </p>
<% else %>
<% end %>